    {
      "name": "HC Overview",
      "description": null,
      "widgets": [
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 1,
            "height": 4,
            "width": 6
          },
          "title": "Throughput",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Log SELECT filter(sum(numeric(capture(message, '.* (?P<Rows>\\\\d+) rows.*'))), WHERE message like '%DATABASE%')*-1 as 'SFDC to PostgreSQL',  filter(sum(numeric(capture(message, '.* (?P<Rows>\\\\d+) rows.*'))), WHERE message like '%SALESFORCE%') as 'PostgreSQL to SFDC' WHERE procid = 'herokuconnect'  since 1 day ago TIMESERIES"
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 7,
            "row": 1,
            "height": 4,
            "width": 6
          },
          "title": "Latency",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Log SELECT filter(average(numeric(capture(message, '.* \\\\((?P<latency>[^ ]*) secs\\\\).*'))), WHERE message like '%DATABASE%')*-1 as 'SFDC to PostgreSQL',  filter(average(numeric(capture(message, '.* \\\\((?P<latency>[^ ]*) secs\\\\).*'))), WHERE message like '%SALESFORCE%') as 'PostgreSQL to SFDC' WHERE procid = 'herokuconnect' AND message like '%SALESFORCE%' since 1 day ago TIMESERIES"
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.bar"
          },
          "layout": {
            "column": 1,
            "row": 5,
            "height": 3,
            "width": 6
          },
          "title": "Latency Avg Ranking (Top 10)",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Log SELECT average(numeric(capture(message, '.* \\\\((?P<latency>[^ ]*) secs\\\\).*'))) WHERE procid = 'herokuconnect' AND message not like '%Informational%' since 1 day ago FACET capture(message, 'event=\"[\\\\d ]*(?P<Table>[^, ]+).*') limit 10"
              }
            ]
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.bar"
          },
          "layout": {
            "column": 7,
            "row": 5,
            "height": 3,
            "width": 6
          },
          "title": "Sync rows sum (Up/Down)",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Log SELECT sum(numeric(capture(message, '.* (?P<Rows>\\\\d+) rows.*'))) WHERE procid = 'herokuconnect' AND message not like '%Informational%' since 1 day ago FACET capture(message, 'event=\"[\\\\d ]*(?P<Table>[^, ]+).*') limit 10"
              }
            ]
          },
          "linkedEntityGuids": null
        },
        {
          "visualization": {
            "id": "viz.table"
          },
          "layout": {
            "column": 1,
            "row": 8,
            "height": 8,
            "width": 12
          },
          "title": "Logs",
          "rawConfiguration": {
            "dataFormatters": [],
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Log   SELECT capture(message, 'event=\"[\\\\d+ ]*(?P<Table>[^ ,]+).*'), capture(message, 'event=\"[\\\\d+ ]*[^ ]+ (?P<Operator>[^ ]+).*'), numeric(capture(message, '.* (?P<Rows>\\\\d+) rows.*')), numeric(capture(message, '.* \\\\((?P<latency>[^ ]*) secs\\\\).*')), message WHERE procid = 'herokuconnect' since 1 day ago limit 50"
              }
            ]
          },
